>

	<changeset id="id1" author="Nick Richter">
		<description>Test changeset 1</description>
		<operations>
			<decomposeTable/>
			<addColumn tableName="table1">
				<column name="column1" type="varchar"/>
			</addColumn>
		</operations>
	</changeset>

